@model Infrastructure.Models.RubroCobro

@{
    ViewBag.Title = "Details";
}

<br />
<div class="container">
    <div class="row justify-content-center">
        <div class="col-md-8">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Detalles de la RubroCobro</h4>
                    <hr />

                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Monto)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Monto)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Descripcion)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Descripcion)
                        </dd>
                    </dl>
                </div>
            </div>
        </div>
        <div class="mt-4">
            @Html.ActionLink("Editar", "Edit", new { id = Model.ID }, new { @class = "btn btn-primary me-2" })
            @Html.ActionLink("Volver al Listado", "Index", null, new { @class = "btn btn-secondary" })
        </div>
    </div>
</div>






